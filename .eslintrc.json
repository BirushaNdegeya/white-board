{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "no-alert": "error",
    "no-unused-vars": "warn",
    "unused-imports/no-unused-imports": "error",
    "indent": ["error", 2]
  },
  "plugins": ["unused-imports"]
}
